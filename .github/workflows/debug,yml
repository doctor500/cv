name: Debug Variable
permissions: write-all

on:
  # Manual Trigger
  workflow_dispatch:


jobs:
  debug_variable:
    runs-on: ubuntu-latest
    steps:
    - name: Print branch name
      run: |
        echo $GITHUB_ENV
        echo "branch=${GITHUB_HEAD_REF:-${GITHUB_REF#refs/heads/}}"
        echo "PIPELINE_TIMESTAMP=$(date +'%s')" >> $GITHUB_ENV
        echo "PIPELINE_DATE=$(date +'%Y%m%d')" >> $GITHUB_ENV